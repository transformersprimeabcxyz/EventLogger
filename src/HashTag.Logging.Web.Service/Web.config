    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <!-- 
    Values can be:

    Critical         - Critical events written to log
    Error            - Critical, Error events written to log
    Warning          - Critical, Error, Warning events written to log
    Information      - Critical, Error, Warning, Information events written to log
    Verbose          - Critical, Error, Warning, Information, Verbose events written to log
    ActivityTracing  - Start, Stop, Transfer events written to log
    All              - Critical, Error, Warning, Information, Verbose, Start, Stop, Transfer events written to log

    Off - NO events written to log

    Combine values using comma delimited list

    Default: Information, ActivityTracing

    for more info see:  http://www.thejoyofcode.com/System.Diagnostics_hidden_SourceLevels.aspx
  -->
  <system.diagnostics>
    <sources>
      <source name="HashTag.Diagnostics.TraceSource" switchValue="All">
        <listeners>
          <add name="HashTagEventServiceListener" type="HashTag.Logging.MSSQL.dbListener, HashTag.Logging.MSSQL" connectionStringName="dbLog"/>
        </listeners>
      </source>
      <source name="HashTag.Diagnostics.TraceSource.FailOver" switchValue="Error">
        <listeners>
          <add name="FileListener" type="HashTag.Diagnostics.TraceListeners.EventBlockFileListener, HashTag.Core" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
</configuration>